{
  "name": "ticketwatcher",
  "description": "チケット表示",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p watch.* start",
    "start": "node dist/server.js",
    "build": "cross-env NODE_ENV=development webpack && tsc",
    "release": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "watch": "run-p watch.*",
    "watch.ts": "nodemon --ext ts --watch src -x tsc",
    "watch.bundle": "nodemon --ext js,html,vue --watch src/public -x cross-env NODE_ENV=development webpack"
  },
  "dependencies": {
    "bootstrap-vue": "^2.0.0-rc.9",
    "compression": "^1.7.2",
    "cookie-parser": "~1.4.3",
    "datatables": "^1.10.13",
    "datatables.net": "^1.10.16",
    "datatables.net-bs4": "^1.10.16",
    "debug": "~2.6.9",
    "express": "~4.16.3",
    "http-errors": "~1.6.3",
    "jquery": "^3.3.1",
    "morgan": "^1.9.0",
    "moment": "2.22.1",
    "request": "^2.86.0",
    "request-promise": "^4.2.2",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-solid": "^5.0.10",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.11.1",
    "@types/nedb": "^1.8.5",
    "@types/jquery": "3.3.2",
    "@types/request": "^2.47.0",
    "@types/request-promise": "^4.1.41",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "browser-sync": "^2.24.1",
    "connect-browser-sync": "^2.1.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "~1.17.2",
    "npm-run-all": "^4.1.2",
    "tar": "^4.4.2",
    "ts-loader": "3.5.0",
    "typescript": "^2.8.3",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}